{
    "/hello/\\w+/world/\\d+": {
        "GET": {
            "status": 200,
            "body": {
                "hello": "world",
                "hi": "there"
            }
        },
        "POST": {
            "status": 200,
            "body": "Hello, World"
        }
    },
    "/greet/[\\w/]*/me/\\w*": {
        "POST": {
            "status": 301,
            "body": {
                "location": "I am moved"
            }
        }
    }
}